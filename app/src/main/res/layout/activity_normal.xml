<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.fleury.marc.moodtracker.view.VerticalViewPager"
    android:background="@color/cornflower_blue_65">

    <ImageView
        android:id="@+id/activity_normal_view"
        android:src="@drawable/smiley_normal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"/>

    <ImageView
        android:id="@+id/activity_normal_hist"
        android:src="@drawable/ic_history_black"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_margin="15dp"/>


    <ImageView
        android:id="@+id/activity_normal_com"
        android:src="@drawable/ic_note_add_black"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_margin="15dp"/>

</RelativeLayout>
